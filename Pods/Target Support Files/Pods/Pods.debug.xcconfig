GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Heap" "${PODS_ROOT}/Headers/Public/Masonry"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/Heap" "$(PODS_ROOT)/Heap"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Heap" -isystem "${PODS_ROOT}/Headers/Public/Masonry"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Heap" -l"Masonry" -l"icucore" -framework "CFNetwork" -framework "Foundation" -framework "Security" -framework "SystemConfiguration" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods